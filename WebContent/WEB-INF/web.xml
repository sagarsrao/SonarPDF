<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>CognizantProjects</display-name>
  <welcome-file-list>
    <welcome-file>Index.html</welcome-file>
    <welcome-file>DisplayProject.jsp</welcome-file>
    <welcome-file>DisplayNew.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>DisplayTable</servlet-name>
    <servlet-class>com.cognizant.servers.DisplayTable</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DisplayTable</servlet-name>
    <url-pattern>/DisplayTable</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>PdfReport</servlet-name>
    <servlet-class>com.cognizant.servers.PdfReport</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PdfReport</servlet-name>
    <url-pattern>/PdfReport</url-pattern>
  </servlet-mapping>
  
  
</web-app>